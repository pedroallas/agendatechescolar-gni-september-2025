{
  "buildCommand": "npx prisma generate && npm run build",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "installCommand": "npm install && npx prisma generate",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
  }
}
